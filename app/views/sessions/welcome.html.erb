<h1>Welcome</h1>


<% if flash.any? %>
  <% flash.each do |type, msg| %>
    <div>
      <%= msg %>
    </div>
    <br>
  <% end %>
<% end %>


<%if logged_in?%>
  <h1> You are Logged In, <%= current_user.username%></h1>
  <%= button_to "Sign out", '/logout', method: :delete%>
<%else%>
  <%= button_to "Login", '/login', method: :get%>
  <%= button_to "Sign up", '/users/new', method: :get%>
<%end%>
