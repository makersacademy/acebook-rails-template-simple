<h1>Login</h1>
<%= render partial: 'posts/flashes' %>
<%= form_tag '/login' do%>
    <%= label_tag :username%>
    <%= text_field_tag :username, nil, :required => true %>
    <%= label_tag :password%>
    <%= password_field_tag :password, nil, :required => true %>
    <%= submit_tag "Login"%>
<%end%>

<p>Don't have an account?</p> <%=button_to "Sign up", '/users/new', method: :get%> 
